{
    "name": "mobilejazz-harmony-typescript",
    "version": "0.0.0",
    "description": "Building blocks for a typescript application based on Harmony concept",
    "author": "Mobile Jazz",
    "homepage": "https://github.com/mobilejazz/harmony-typescript",
    "repository": {
        "type": "git",
        "url": "https://github.com/mobilejazz/harmony-typescript.git"
    },
    "license": "Apache-2.0",
    "readmeFilename": "README.md",
    "bugs": {
        "url": "https://github.com/mobilejazz/harmony-typescript/issues"
    },
    "engines": {
        "node": "~10.13",
        "npm": "~6.4"
    },
    "scripts": {
        "prebuild": "npm run validate",
        "build": "lerna run build",
        "validate": "lerna run lint",
        "publish": "npm run build && lerna publish --contents dist"
    },
    "dependencies": {
        "@mobilejazz/harmony-core": "^0.0.3"
    },
    "devDependencies": {
        "@types/node": "~11.12.0",
        "husky": "~1.3.1",
        "lerna": "^3.13.4",
        "typescript": "~3.1.6"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run validate",
            "pre-push": "npm run validate"
        }
    }
